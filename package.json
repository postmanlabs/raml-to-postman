{
  "name": "raml-to-postman",
  "version": "2.1.0",
  "description": "Convert a given RAML spec to Postman spec",
  "main": "index.js",
  "engines": {
    "node": ">=16"
  },
  "com_postman_plugin": {
    "plugin_type": "importer",
    "name": "RAML 0.8 to Postman",
    "source_format": "RAML0.8",
    "source_format_name": "RAML 0.8"
  },
  "bin": {
    "raml2postman": "./bin/raml2postman"
  },
  "dependencies": {
    "async": "^3.2.4",
    "commander": ">=2.15.1",
    "lodash": "4.17.21",
    "path-browserify": "^1.0.1",
    "postman_validator": ">= 0.0.4",
    "raml-parser": "0.8.18",
    "uuid": ">=3.2.1"
  },
  "author": "Postman Labs <help@getpostman.com>",
  "license": "Apache",
  "devDependencies": {
    "chai": "4.3.6",
    "mocha": "10.2.0",
    "nyc": "15.1.0"
  },
  "scripts": {
    "release": "./scripts/release.sh",
    "test": "./scripts/test.sh",
    "test-unit": "./scripts/test-unit.sh"
  }
}
